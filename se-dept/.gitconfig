[user]
	name = Andy Meneely
	email = andy@se.rit.edu
[alias]
        acp = !git add -A . && git commit && git push
	done = !git checkout master && git merge dev && git push --all && git checkout dev
     	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
        lgsearch = "!f() {git log -w -b --ignore-space-at-eol --ignore-space-change --ignore-all-space --name-status --pretty=short -S'$1'}; f"
     	st = status
     	co = checkout
	ignore=!([ ! -e .gitignore ] && touch .gitignore) | echo $1 >>.gitignore
[color]
	ui = true
